<launch>
    <node name="controller" pkg="navigation_test" type="navigation_test.py" output='screen'>
        <param name="~debug" value="True" />  <!-- pass Simulation -->
        <param name="~Pass_Voice" value="False" />  <!-- pass Voice -->
    </node>
    <!-- <include file="$(find ucar_nav)/launch/ucar_navigation.launch"/> -->
    <include file="$(find ucar_nav)/launch/ucar_navigation0702.launch"/>

    <!-- <include file="$(find speech_command)/launch/speech_command.launch"/> -->
    <include file="$(find rknn_ros)/launch/rknn.launch"/>

    <!-- <include file="$(find lidar_undistortion)/launch/lidar_undistortion_offline.launch"/> -->
        


    <node name="visual_nav" pkg="visual_navigation" type="visual_navigation" output="log" >
        <param name="highthreshold" value="95" />
        <param name="lowthreshold" value="50" />
        <param name="vel_max_" value="0.75" />
        <param name="acceleration_" value="0.15" />
        <param name="vel_start_" value="0.3" />
        <param name="KP" value="21" />
        <param name="KI" value="0" />
        <param name="KD" value="65.0" />
        <param name="VIDEO_OPEN_" value="true" />
        <param name="video_path" value="/home/ucar/Videos/" />
        <param name="Avoid_Dist" value="0.45" /> 
        <param name="Trace_edge" value="0" />
        <param name="Debug" value="true" />
    </node> 
</launch>



